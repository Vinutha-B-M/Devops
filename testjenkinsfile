pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                // your build steps
            }
        }
    }
    post {
        always {
            // add a batch command to commit changes and push to GitHub
            bat 'git add .'
            bat 'git commit -m "pipeline build successful"'
            bat 'git push origin master'
        }
    }
}
